<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <record id="seq_venue_booking" model="ir.sequence">
            <field name="name">Booked Sequence</field>
            <field name="code">venue.booking.sequence</field>
            <field name="prefix">Book/%(range_year)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

         <record id="seq_venue_payment" model="ir.sequence">
            <field name="name">Payment Sequence</field>
            <field name="code">venue.payment.sequence</field>
            <field name="prefix">PYM/%(range_year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>